<p id="notice"><%= notice %></p>

<h1>informacoes</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @informacoes.each do |informacao| %>
      <tr>
        <td><%= informacao.tipo %></td>
        <td><%= link_to 'Show', informacao %></td>
        <td><%= link_to 'Edit', edit_informacao_path(informacao) %></td>
        <td><%= link_to 'Destroy', informacao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New informacao', new_informacao_path %>
